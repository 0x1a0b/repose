<?xml version="1.1" encoding="UTF-8"?>

<atom-feed-service xmlns="http://docs.openrepose.org/repose/atom-feed-service/v1.0"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

    <feed id="non-authenticating-feed" uri="http://public.atom.com/feed"/>

    <!-- when the page of new entries is passed to the service, they will be passed in reverse order -->

    <feed id="authenticating-feed" uri="http://private.atom.com/feed" connection-pool-id="default">
        <authentication xsi:type="OpenStackIdentityV2AuthenticationType"
                        timeout="5000"
                        uri="http://identity.jware.com"
                        username="zombiekid"
                        password="iliketurtles"/>
    </feed>
</atom-feed-service>

<?xml version="1.0" encoding="UTF-8"?>

<url-extractor-to-header xmlns="http://docs.openrepose.org/repose/url-extractor-to-header/v1.0">
    <!-- Grab the Tenant ID from the URI path segment immediately following servers; don't include header if URL doesn't match -->
    <extraction header="X-Expected-Tenant" url-regex=".*/servers/([:\-\w]+)/?.*"/>
</url-extractor-to-header>

<?xml version="1.0" encoding="UTF-8"?>
<keystone-v3 xmlns="http://docs.openrepose.org/keystone-v3/v1.0"
        forward-groups="false"
        token-cache-timeout="1">

    <keystone-service username="admin-username"
                      password="admin-password"
                      uri="http://localhost:${identityPort}"
                      xmlns="http://docs.openrepose.org/keystone-v3/v1.0"/>
    <service-endpoint url="http://localhost:${targetPort}"/>
</keystone-v3>
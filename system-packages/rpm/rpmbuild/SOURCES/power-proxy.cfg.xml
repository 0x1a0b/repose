<?xml version="1.0" encoding="UTF-8"?>

<power-proxy xmlns="http://docs.rackspacecloud.com/power-api/system-model/v1.0">
  <host id="papi-n01" hostname="localhost" service-port="65000">
        <filters>
            <filter name="client-auth" />
            <filter name="versioning" />
            <filter name="dist-datastore" />
            <filter name="rate-limiting" />
        </filters>
    </host>
    
    <host id="service-v1" hostname="web-n01.localhost" href="http://localhost:8080/v1" />
    <host id="service-v2" hostname="web-n02.localhost" href="http://localhost:8080/v2" />
</power-proxy>

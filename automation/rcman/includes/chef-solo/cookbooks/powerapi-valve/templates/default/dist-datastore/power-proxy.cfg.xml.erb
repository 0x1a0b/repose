<?xml version="1.0" encoding="UTF-8"?>

<power-proxy xmlns="http://docs.rackspacecloud.com/power-api/system-model/v2.0">
  <service-domain id="repose">

    <service-domain-nodes>
      <node id="proxy-n01" hostname="<%= node[:fqdn]%>" http-port="8890"/>
      <node id="proxy-n02" hostname="<%= node[:fqdn]%>" http-port="8891"/>
      <node id="proxy-n03" hostname="<%= node[:fqdn]%>" http-port="8892"/>
    </service-domain-nodes>

    <filters>
        <filter name="dist-datastore"/>
    </filters>

    <destinations>
      <endpoint id="service" protocol="http" hostname="50.57.189.15" root-path="/" port="80" default="true"/>
    </destinations>

  </service-domain>
</power-proxy>
